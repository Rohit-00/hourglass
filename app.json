{
  "expo": {
    "name": "Hourglass",
    "slug": "Hourglass",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/hourglass.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/hourglass.png",
      "resizeMode": "contain",
      "backgroundColor": "#407AFF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/hourglass.png",
        "backgroundColor": "#407AFF"
      },
      "package": "com.rohit965.tickdown"
    },
    "web": {
      "favicon": "./assets/hourglass.png"
    },
    "plugins": [
      "expo-sqlite"
    ]
  }
}
